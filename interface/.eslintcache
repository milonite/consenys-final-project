ges/buy_blankets.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/pages/market.tsx",["128","129"],"import React, { useState } from \"react\";\nimport ArtCard from \"../components/Cards/BuyArtCard\";\nimport { Grid, Typography } from \"@material-ui/core\";\nimport { useOkazzPollock, useArtPieceOne } from \"../hooks/useContract\";\nimport { shortenAddress } from \"../utils\";\nimport { useWeb3React } from \"@web3-react/core\";\nimport { Web3Provider } from \"@ethersproject/providers\";\n\nfunction Buy() {\n  const web3React = useWeb3React<Web3Provider>();\n\n  const contractPollock = useOkazzPollock();\n  const contractBlankets = useArtPieceOne();\n  const [totalBlankets, setTotalBlankets] = useState(\"\");\n  const [totalPollock, setTotalPollock] = useState(\"\");\n  const [ownerPollock, setOwnerPollock] = useState(\"\");\n  const [ownerBlankets, setOwnerBlankets] = useState(\"\");\n  const { chainId } = web3React;\n\n  React.useEffect(() => {\n    if (contractPollock && chainId === 4) {\n      const getTotal = async () => {\n        const total = await contractPollock.totalSupply();\n        setTotalPollock(total.toNumber());\n        const owner = await contractPollock.owner();\n        setOwnerPollock(shortenAddress(owner));\n      };\n      getTotal();\n    }\n  }, [chainId]);\n\n  React.useEffect(() => {\n    if (contractBlankets && chainId === 4) {\n      const getTotal = async () => {\n        const total = await contractBlankets.totalSupply();\n        setTotalBlankets(total.toNumber());\n        const owner = await contractBlankets.owner();\n        setOwnerBlankets(shortenAddress(owner));\n      };\n      getTotal();\n    }\n  }, [chainId]);\n\n  return (\n    <div>\n      <Typography variant=\"h6\">MARKET </Typography>\n      <Grid\n        container\n        spacing={3}\n        direction=\"row\"\n        justify=\"center\"\n        alignItems=\"center\"\n      >\n        <Grid item>\n          <ArtCard\n            to={\"/createBlankets\"}\n            title={\"Blankets\"}\n            total={totalBlankets}\n            imageUrl=\"https://i.ibb.co/3c9CxZT/canvas.png\"\n            author=\"Kgolid\"\n            owner={ownerBlankets}\n          ></ArtCard>\n        </Grid>\n        <Grid item>\n          <ArtCard\n            to={\"/createPollock\"}\n            title={\"Pollock\"}\n            total={totalPollock}\n            imageUrl=\"https://i.ibb.co/TYN8qsn/canvas-Okazz.png\"\n            author=\"Okazz\"\n            owner={ownerPollock}\n          ></ArtCard>\n        </Grid>\n      </Grid>\n    </div>\n  );\n}\n\nexport default Buy;\n","/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/pages/buy_pollock.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/pages/my_tokens.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/assets/fonts/index.js",[],["130","131"],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/hooks/useContract.ts",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/hooks/index.ts",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/constants/index.ts",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/connectors/injected.ts",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/utils/index.ts",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/components/Animations/Sparkle.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/hooks/usePreferedReducedMotion.ts",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/hooks/useRandomInterval.ts",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/components/Animations/utils/index.ts",[],["132","133"],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/components/Arts/kgolid_blankets/KGolidBlankets.tsx",[],"/Users/taa