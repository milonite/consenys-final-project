     };\n      getOwner();\n    }\n  }, [contractBlankets, account]);\n\n  const handleSelect = (art: string, tokenId: string) => {\n    setSelected({\n      art: art,\n      tokenId: tokenId,\n    });\n  };\n\n  const handleClose = () => {\n    setSelected({\n      art: null,\n      tokenId: null,\n    });\n  };\n\n  return (\n    <div>\n      <Dialog\n        handleClose={handleClose}\n        open={selected.art ? true : false}\n        selected={selected}\n      ></Dialog>\n      {tokenIdsPollock.length > 0 && tokenIdsBlankets.length > 0 && (\n        <Typography variant=\"h6\"> MY TOKENS </Typography>\n      )}\n\n      {active === false || chainId !== 4\n        ? \"Please connect to the Rinkeby Network\"\n        : [\n            loading ? (\n              <LoadingBar></LoadingBar>\n            ) : (\n              <>\n                <Grid\n                  container\n                  direction=\"row\"\n                  justify=\"center\"\n                  alignItems=\"center\"\n                >\n                  {tokenIdsPollock.map((el) => (\n                    <Grid item>\n                      <OverviewCard\n                        title={\"Pollock\"}\n                        author={\"Okazz\"}\n                        tokenId={el}\n                        handleSelect={handleSelect}\n                        owner={ownerPollock}\n                        imageUrl={\"https://i.ibb.co/TYN8qsn/canvas-Okazz.png\"}\n                      ></OverviewCard>\n                    </Grid>\n                  ))}\n                  {tokenIdsBlankets.map((el) => (\n                    <Grid item>\n                      <OverviewCard\n                        title={\"Blankets\"}\n                        author={\"Kgolid\"}\n                        tokenId={el}\n                        owner={ownerBlankets}\n                        handleSelect={handleSelect}\n                        imageUrl=\"https://i.ibb.co/3c9CxZT/canvas.png\"\n                      ></OverviewCard>\n                    </Grid>\n                  ))}\n                </Grid>\n              </>\n            ),\n          ]}\n\n      <Typography variant=\"h6\"> MY MACHINES </Typography>\n      <Grid container direction=\"row\" justify=\"center\" alignItems=\"center\">\n        {isBlanketsOwner && (\n          <Grid item>\n            <MyMachine\n              to={\"/createBlankets\"}\n              title={\"Blankets\"}\n              total={totalBlankets}\n              imageUrl=\"https://i.ibb.co/3c9CxZT/canvas.png\"\n              author=\"Kgolid\"\n            ></MyMachine>\n          </Grid>\n        )}\n        {isPollockOwner && (\n          <Grid item>\n            <MyMachine\n              to={\"/createPollock\"}\n              title={\"Pollock\"}\n              total={totalPollock}\n              imageUrl=\"https://i.ibb.co/TYN8qsn/canvas-Okazz.png\"\n              author=\"Okazz\"\n            ></MyMachine>\n          </Grid>\n        )}\n      </Grid>\n    </div>\n  );\n}\n\nexport default TokenList;\n","/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/components/Cards/OverviewCard.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/components/Dialogs/Dialog.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/components/Loading/LoadingBar.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/pages/landing.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/pages/about.tsx",[],"/Users/taacomi8/Projects/Personal/consenys-final-project/interface/src/components/Cards/MyMachine.tsx",[],{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","replacedBy":"139"},{"ruleId":"140","severity":1,"message":"141","line":30,"column":6,"nodeType":"142","endLine":30,"endColumn":15,"suggestions":"143"},{"ruleId":"140","severity":1,"message":"144","line":42,"column":6,"nodeType":"142","endLine":42,"endColumn":15,"suggestions":"145"},{"rul